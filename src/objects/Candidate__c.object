    <validationRules>
        <fullName>EducationNotNullRule</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Education__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Education__c</errorDisplayField>
        <errorMessage>You must select an education level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StateNotNullRule</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(State__c, &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>State__c</errorDisplayField>
        <errorMessage>You must select a state</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code_Rule</fullName>
        <active>false</active>
        <description>Zip code must be in 99999 or 99999-9999 format.</description>
        <errorConditionFormula>NOT(REGEX(  Zip_Code__c  , &quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
        <errorDisplayField>Zip_Code__c</errorDisplayField>
        <errorMessage>Zip code must be in 99999 or 99999-9999 format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>current_employee_rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISBLANK (  Current_Employer__c ), 
ISBLANK(   Year_of_Experience__c  ),

OR(  Currently_Employed__c )
)</errorConditionFormula>
        <errorMessage>Current Employee must be specified when the Currently Employed is checked.</errorMessage>
    </validationRules>
